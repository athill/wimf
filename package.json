{
  "name": "wimf",
  "version": "0.0.1",
  "description": "What's in my freezer?",
  "repository": "https://github.com/athill/wimf",
  "license": "MIT",
  "config": {
    "buildDir": "./public",
    "buildDirTests": "./build_tests",
    "devHost": "0.0.0.0",
    "devPort": 8000
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./resources/assets/app.js ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config",
    "watch": "NODE_ENV=development webpack --colors --progress --config ./webpack/config --watch"
  },
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "axios": "^0.9.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.3",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "expect": "^1.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.1.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "laravel-elixir": "^5.0.0",
    "lodash": "^4.3.0",
    "mocha": "^2.4.5",
    "moment": "^2.12.0",
    "phantomjs-prebuilt": "^2.1.4",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-datepicker": "^0.24.0",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.2.4",
    "react-router-bootstrap": "^0.22.1",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-form": "^4.1.14",
    "redux-logger": "^2.5.2",
    "redux-mock-store": "0.0.6",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "template-html-loader": "0.0.3",
    "webpack": "^1.10.1"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "globals": {
      "describe": true,
      "it": true,
      "sinon": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "strict": [
        2,
        "global"
      ],
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "no-alert": 0,
      "no-underscore-dangle": 0,
      "react/display-name": 0,
      "react/jsx-quotes": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-sort-props": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 1,
      "react/no-unknown-property": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/wrap-multilines": 1
    }
  },
  "devDependencies": {
    "karma-chrome-launcher": "^0.2.3",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0"
  }
}
