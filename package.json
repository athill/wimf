  "scripts": {
    "start": "node resources/assets/scripts/start.js",
    "build": "node resources/assets/scripts/build.js",
    "test": "node resources/assets/scripts/test.js --env=jsdom",
    "watch": "NODE_ENV=production webpack --config resources/assets/config/webpack.config.prod.js --progress --colors --watch"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "resources/assets/src/**/*.js",
      "!**/src/app.js",
      "!**/src/redux/create.js"
    ],
    "coverageDirectory": "public/jscoverage",
    "coverageReporters": [
      "html"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/resources/assets/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
